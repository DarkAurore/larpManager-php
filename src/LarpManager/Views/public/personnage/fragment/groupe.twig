{# Groupe principal #}
<div class="header">
	<h5>Groupe principal</h5>
</div>

<div class="row">
	<div class="col-sm-6">
		<ul class="list-group">
			{% if personnage.groupe %}
				<li class="list-group-item"><a href="{{ path('participant.groupeGn.groupe', {'groupeGn': participant.session.id}) }}">{{ participant.session.groupe.nom }}</a></li>
			{% else %}
				<li class="list-group-item">N'est pas lié à un groupe.</li>
			{% endif %}			
		</ul>
	</div>
</div>